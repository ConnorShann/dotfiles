
#other
alias c='clear'
alias vim='nvim'

#ls
alias ls='ls --color=auto'
alias ll='ls -la --color=auto'

#git
alias grests='git restore --staged'
alias grest='git restore'
alias gadd='git add'
alias gb='git branch'
alias gc='git checkout'
alias gcp='git cherry-pick'
alias gcm='git commit -m'
alias gfom='git fetch origin main mainline master'
alias gs='git status'
alias gl='git log'
alias grl='git log'
alias gri='gir rebase -i'
alias gf='git fetch'
alias gfr='git fetch --rebase'
alias gdl='git diff --staged'

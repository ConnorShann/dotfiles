if [ -f $HOME/.config/bash/settings ]; then
	. $HOME/.config/bash/settings
fi

if [ -f $HOME/.config/bash/aliases ]; then
	. $HOME/.config/bash/aliases
fi

if [ -f $HOME/.local_custom_bashrc ]; then
	. $HOME/.local_custom_bashrc
fi

[ -f ~/.fzf.bash ] && source ~/.fzf.bash
[ -f ~/.config/bash/.exports ] && source ~/.config/bash/.exports
[ -f ~/.config/bash/.plugins ] && source ~/.config/bash/.plugins
